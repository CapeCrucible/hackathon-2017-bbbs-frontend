// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/**.js.map": true,
        "**/*.js": {
            "when": "$(basename).ts"
        },
        "**/*.log": true,
        "**/*.log.*": true,
        "**/*.nuspec": true,
        "**/jspm_packages": true,
        "**/node_modules": true,
        "**/typings": true,
        "**/fonts": true,
        "dist/": true
    },
    "atomic_save": true,
    "typescript.check.workspaceVersion": false,
    "vsicons.presets.angular": true
}